{
  "id": "ONTOLOGY_DRAFT",
  "name": "Ontology Draft",
  "description": "Organizational hierarchy draft from ontology-setup skill",
  "file_types": ["json"],
  "render": {
    "component": "OntologyDraftViewer",
    "supports_preview": true,
    "interactive": true
  },
  "metadata_schema": {
    "type": "object",
    "required": ["structure_type"],
    "properties": {
      "structure_type": {
        "type": "string",
        "enum": ["flat", "functional", "divisional", "product_centric", "matrix"]
      },
      "version": {
        "type": "integer",
        "minimum": 1
      }
    }
  }
}
