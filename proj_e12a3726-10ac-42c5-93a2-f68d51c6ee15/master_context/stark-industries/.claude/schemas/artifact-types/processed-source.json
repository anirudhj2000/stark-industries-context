{
  "id": "PROCESSED_SOURCE",
  "name": "Processed Source",
  "description": "Processed source documents",
  "file_types": ["md", "json"],
  "render": {
    "component": "DocumentViewer",
    "supports_preview": true,
    "interactive": false
  },
  "metadata_schema": {
    "type": "object",
    "required": ["source_type"],
    "properties": {
      "source_type": {
        "type": "string",
        "enum": ["transcript", "document", "meeting", "extraction"]
      },
      "duration_seconds": {
        "type": "integer"
      },
      "word_count": {
        "type": "integer"
      },
      "language": {
        "type": "string",
        "default": "en"
      }
    }
  }
}
