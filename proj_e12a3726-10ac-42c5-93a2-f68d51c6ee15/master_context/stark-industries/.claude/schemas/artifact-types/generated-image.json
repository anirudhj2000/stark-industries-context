{
  "id": "GENERATED_IMAGE",
  "name": "Generated Image",
  "description": "AI-generated images (diagrams, charts, illustrations)",
  "file_types": ["png", "jpg", "svg", "webp"],
  "render": {
    "component": "ImageViewer",
    "supports_preview": true,
    "interactive": false
  },
  "metadata_schema": {
    "type": "object",
    "required": [],
    "properties": {
      "prompt": {
        "type": "string",
        "description": "The prompt used to generate the image"
      },
      "dimensions": {
        "type": "object",
        "properties": {
          "width": {"type": "integer"},
          "height": {"type": "integer"}
        }
      },
      "aspect_ratio": {
        "type": "string",
        "enum": ["16:9", "4:3", "1:1", "9:16"],
        "default": "16:9"
      },
      "style": {
        "type": "string"
      },
      "source_api": {
        "type": "string",
        "description": "API used (gemini, dall-e-3, etc.)"
      }
    }
  }
}
